{
    "mule.runtime.defaultArguments": [
        "-M-Dhttp.port=8081",
        "-M-Dhttps.port=8082",
        "-M-Dmcp.serverName=CMS-Demo-MCP-Server",
        "-M-Dmcp.serverVersion=1.0.0"
    ],
    "files.exclude": {
        "**/target": true,
        "**/.mule": true
    },
    "files.associations": {
        "*.xml": "xml"
    },
    "xml.validation.enabled": true,
    "xml.format.enabled": true
}
